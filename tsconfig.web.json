{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/**/*", 
    "electron/**/*", 
    "playwright-ct.config.ts", 
    "setupTests.js",
    "tests-examples/**/*",
    "playwright/**/*",
    "e2e-testing/**/*"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      
    }
  }
}